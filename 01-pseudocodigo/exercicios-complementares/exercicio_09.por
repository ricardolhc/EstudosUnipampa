programa
{
	funcao inicio()
	{
		real a, b, c

		escreva("Digite o primeiro valor: ")
		leia(a)

		escreva("Digite o segundo valor: ")
		leia(b)

		escreva("Digite o terceiro valor: ")
		leia(c)

		escreva("O resultado da expressão (", a ," - ", c ,") * ", b ," é: ", (a - c) * b)
	}
}
