programa
{
	funcao inicio()
	{
		inteiro anos_trabalho, bonus_salario
		
		escreva("Digite a quantidade de anos que o funcionário trabalhou: ")
		leia(anos_trabalho)

		bonus_salario = (anos_trabalho / 5) * 2

		escreva("O trabalhador que trabalhou ", anos_trabalho ," ano(s), recebeu um bônus de ", bonus_salario
		," salários")
	}
}
